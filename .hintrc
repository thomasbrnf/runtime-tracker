{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "typescript-config/is-valid": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "font-synthesis",
          "-webkit-tap-highlight-color",
          "backdrop-filter"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ]
  }
}